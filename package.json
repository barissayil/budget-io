    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
